        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Movement

if global.camera_panning = 0

{
    //React to inputs
    move = key_left + key_right;
    hsp = move * 4; //movespeed is 4
    if (vsp &lt; 10) vsp += grav;

    //Horizontal Collision
    if (place_meeting(x+hsp,y,obj_barrier))
    {
    while(!place_meeting(x+sign(hsp),y,obj_barrier))
        {
            x += sign(hsp);
        }
        hsp = 0;
    }
    //Horizontal Collison with BAD OBJECTS
    if (place_meeting(x+hsp,y,obj_bad))
    {
    while(!place_meeting(x+sign(hsp),y,obj_bad))
        {
            x += sign(hsp);
        }
        hsp = 0;
            if global.invuln = 0{
                with (obj_bad) event_perform(ev_other,ev_user0);
                }
    }

    x += hsp;

    //Vertical Collision
    if (place_meeting(x,y+vsp,obj_barrier))
    {
    while(!place_meeting(x,y+sign(vsp),obj_barrier))
        {
            y += sign(vsp);
        }
        vsp = 0;

    }
    
        //Vertical Collision with BAD OBJECTS
    if (place_meeting(x,y+vsp,obj_bad))
    {
    while(!place_meeting(x,y+sign(vsp),obj_bad))
        {
            y += sign(vsp);
        }
        vsp = 0;
        
        if global.invuln = 0{
                with (obj_bad) event_perform(ev_other,ev_user0);
                }
        
    }
    

    //check if you are on the ground to grant you more jumps
    if place_meeting(x,y+1,obj_barrier) or place_meeting(x,y+1,obj_bad)
    {
        jumpcount = 2;
    }

    y += vsp;

}
</string>
